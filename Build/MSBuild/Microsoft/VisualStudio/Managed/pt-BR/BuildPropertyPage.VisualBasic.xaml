<Rule Name="Build" OverrideMode="Replace" Description="Especifica as propriedades que controlam como o projeto é compilado." DisplayName="Compile" PageTemplate="generic" Order="200" xmlns="http://schemas.microsoft.com/build/2009/properties">
  <Rule.Categories>
    <Category Name="General" DisplayName="Geral" />
    <Category Name="Options" DisplayName="Options" />
    <Category Name="Warnings" DisplayName="Warnings" />
    <Category Name="Events" Description="Configura eventos personalizados que são executados antes e depois do build." DisplayName="Eventos" />
    <Category Name="Advanced" DisplayName="Avançado" Description="Configurações avançadas do aplicativo." />
